{
  "name": "sqlwayfarer",
  "displayName": "SQL Wayfarer",
  "description": "Navigate and explore SQL Server databases directly within VS Code. View tables, procedures, dependencies, and more.",
  "version": "0.6.11",
  "publisher": "intarynx",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "sql",
    "tsql",
    "mssql",
    "database",
    "explorer",
    "dependencies",
    "schema",
    "procedure",
    "table"
  ],
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/intarynx/sqlwayfarer.git"
  },
  "bugs": {
    "url": "https://github.com/intarynx/sqlwayfarer/issues"
  },
  "homepage": "https://github.com/intarynx/sqlwayfarer",
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sqlwayfarer.sqlwayfarer",
        "title": "Open SQL Wayfarer",
        "icon": "$(compass)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "sql-wayfarer-container",
          "title": "SQL Wayfarer",
          "icon": "$(compass)"
        }
      ]
    },
    "views": {
      "sql-wayfarer-container": [
        {
          "id": "sqlWayfarerView",
          "name": "SQL Wayfarer"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "sqlwayfarer.sqlwayfarer",
          "when": "view == sqlWayfarerView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "build": "webpack",
    "package": "npm run build && vsce package",
    "publish": "npm run build && vsce publish"
  },
  "dependencies": {
    "mssql": "^10.0.1"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.100.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.1.6",
    "webpack": "^5.101.0",
    "webpack-cli": "^5.1.4"
  }
}
